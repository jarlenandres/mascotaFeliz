@page
@model MascotaFeliz.App.Frontend.Pages.DetallesVisitasPyPModel
@{
}

<div class="containet">
    <h4>Detalles de la VisitaPyP</h4>
    <div class="row">
        <table class="table table-striped table-hover">
            <div>
                Id: @Model.visitaPyP.Id
            </div>
            <div>
                Fecha Visita: @Model.visitaPyP.FechaVisita
            </div>
            <div>
                Temperatura: @Model.visitaPyP.Temperatura
            </div>
            <div>
                Peso: @Model.visitaPyP.Peso
            </div>
            <div>
                FrecuenciaRespiratoria : @Model.visitaPyP.FrecuenciaRespiratoria
            </div>
            <div>
                FrecuenciaCardiaca: @Model.visitaPyP.FrecuenciaCardiaca
            </div>
            <div>
                EstadoAnimo: @Model.visitaPyP.EstadoAnimo
                <div>
                    Recomendaciones: @Model.visitaPyP.Recomendaciones
                </div>
        </table>
    </div>
</div>
<a class="btn btn-secondary" asp-page="./ListaVisitasPyP"> Volver a lista </a>