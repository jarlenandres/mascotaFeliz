@page
@model MascotaFeliz.App.Frontend.Pages.DetallesVeterinarioModel
@{
}

<div class="containet">
    <h4>Detalles del veterinario</h4>
    <div class="row">
        <table class="table table-striped table-hover">
            <div>
                Id: @Model.veterinario.Id
            </div>
            <div>
                Nombre: @Model.veterinario.Nombres
            </div>
            <div>
                Apellidos: @Model.veterinario.Apellidos
            </div>
            <div>
                Direccion: @Model.veterinario.Direccion
            </div>
            <div>
                Telefono: @Model.veterinario.Telefono
            </div>
            <div>
                Tarjeta Profesional: @Model.veterinario.TarjetaProfesional
            </div>
        </table>
    </div>
</div>
<a asp-page=".\ListaVeterinarios" class="btn btn-secondary">
    Volver a la lista
</a>