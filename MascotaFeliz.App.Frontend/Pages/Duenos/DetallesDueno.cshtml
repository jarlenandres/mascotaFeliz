@page
@model MascotaFeliz.App.Frontend.Pages.DetallesDuenoModel
@{
}
<div class="container">
<h4>Detalles del dueño</h4>
    <div class="row">
        <table class="table table-striped table-hover">
            <div>
                Id: @Model.dueno.Id
            </div>
            <div>
                Nombre: @Model.dueno.Nombres
            </div>
            <div>
                Apellidos: @Model.dueno.Apellidos
            </div>
            <div>
                Direccion: @Model.dueno.Direccion
            </div>
            <div>
                Telefono: @Model.dueno.Telefono
            </div>
            <div>
                Correo: @Model.dueno.Correo
            </div>
        </table>
    </div>
</div>
<a asp-page=".\ListaDuenos" class="btn btn-secondary">Volver a la lista</a>